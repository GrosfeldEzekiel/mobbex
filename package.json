  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "chai": "^4.2.0",
